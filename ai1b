n = int(input("enter number"))
Count = 0
for i in range(1, n + 1):
    if n % i == 0:
        Count = Count + 1
if Count == 2:
    print("n is prime number")
else:
    print("n is not a prime number")

def prime(n):
    Count = 0
    for i in range(1, n + 1):
        if n % i == 0:
            Count = Count + 1
    if Count == 2:
        print("prime number")
    else:
        print("not prime number")

n = int(input("enter a number"))
prime(n)
